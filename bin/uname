#!/bin/sh

if [ $# -gt 0 ]; then
    case "$1" in
        -s)
            echo "AROS"
            ;;
        -n)
            echo "localhost"
            ;;
        -r)
            echo "53.18"
            ;;
        -v)
            echo "3.5"
            ;;
        -m)
            echo "i386"
            ;;
        -p)
            echo "FIXME"
            ;;
        -i)
            echo "FIXME"
            ;;
        -a)
            echo "AROS localhost FIXME AROS"
            ;;
        *)
            echo "Illegal option"
            ;;
    esac
else
    echo "AROS"
fi
